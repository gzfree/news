{
  "version": 2,
  "builds": [
    { "src": "**/*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/dashboard", "dest": "/data-analysis/d-income-comparison.html" },
    { "src": "/voip", "dest": "/data-analysis/d-voip.html" },
    { "src": "/", "dest": "/interactive-prototype/index.html" },
    { "src": "/news", "dest": "/interactive-prototype/news.html" },
    { "src": "/search", "dest": "/interactive-prototype/search.html" },
    { "src": "/call", "dest": "/interactive-prototype/noise-cancellation-en.html" },
    { "src": "/filters", "dest": "/interactive-prototype/video-filters.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "github": {
    "silent": true,
    "autoAlias": true
  }
} 